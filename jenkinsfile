pipeline {
         agent any
         stages {
                 stage('clean') {
                 steps {
                     sh './gradlew clean'
                 }
                 }
                 stage('build') {
                 steps {
                    echo "build"
                 }
                 }
                 stage('test') {
                 when {
                       not {
                            branch "master"
                       }
                 }
                 steps {
                       echo "Test"
                 }
                 }
       
              }
}
